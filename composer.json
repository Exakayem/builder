{
    "name": "fluidtypo3/builder",
    "description": "The builder package from FluidTYPO3",
    "type": "typo3-cms-extension",
    "version": "0.15.0",
    "require-dev": {
        "typo3/cms": "6.2.7",
        "fluidtypo3/development": "dev-master"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "typo3/cms",
                "version": "6.2.7",
                "dist": {
                    "url": "https://github.com/TYPO3/TYPO3.CMS/archive/6.2.7.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "FluidTYPO3\\Builder\\": "Classes/"
        },
        "classmap": [
            "vendor/typo3/cms/typo3/sysext"
        ]
    }
}
