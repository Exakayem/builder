{
    "name": "fluidtypo3/builder",
    "description": "The builder package from FluidTYPO3",
    "type": "typo3-cms-extension",
    "replace": {
        "builder": "self.version",
        "typo3-ter/builder": "self.version"
    },
    "require": {
        "php": ">=7.0.0",
        "fluidtypo3/vhs": ">=2.2.0||dev-development",
        "typo3/cms-core": "^8.7||^9||^10",
        "typo3/cms-backend": "^8.7||^9||^10",
        "typo3/cms-fluid": "^8.7||^9||^10"
    },
    "require-dev": {
        "fluidtypo3/development": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "FluidTYPO3\\Builder\\": "Classes/"
        }
    },
    "non-feature-branches": ["development", "staging"]
}
